#include <stdio.h>

int main() {

    printf("O tamanho de um char eh %d byte(s)\n", sizeof(char));
    printf("O tamanho de um int eh %d byte(s)\n", sizeof(int));
    printf("O tamanho de um float eh %d byte(s)\n", sizeof(float));
    printf("O tamanho de um double eh %d byte(s)\n", sizeof(double));
    
return 0;
}
